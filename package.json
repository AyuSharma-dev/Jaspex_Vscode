{
    "name": "jsontoapex",
    "displayName": "JSON To APEX",
    "description": "Convert Json into Salesforce Apex JSON generator class.",
    "version": "0.2.0",
    "author": {
        "name": "Ayush Sharma"
    },
    "publisher": "AyuSharma-dev",
    "engines": {
        "vscode": "^1.42.0"
    },
    "keywords": [
        "salesforce",
        "apex",
        "json",
        "json generator",
        "convert"
    ],
    "categories": [
        "Formatters",
        "Programming Languages",
        "Snippets"
    ],
    "icon": "Images/icon.png",
    "activationEvents": [
        "onCommand:extension.generateapex",
        "onCommand:extension.generateapexfromcopy"
    ],
    "galleryBanner": {
        "color": "black",
        "theme": "dark"
    },
    "main": "./extension.js",
    "contributes": {
        "languages": [
            {
                "id": "Apex",
                "aliases": [
                    "Apex",
                    "Apex"
                ],
                "extensions": [
                    ".cls"
                ]
            }
        ],
        "commands": [
            {
                "command": "extension.generateapex",
                "title": "JSON To APEX: Create JSON Generator Object From Selected Text"
            },
            {
                "command": "extension.generateapexfromcopy",
                "title": "JSON To APEX: Create JSON Generator Object From Copied JSON."
            }
        ]
    },
    "scripts": {
        "lint": "eslint .",
        "pretest": "npm run lint",
        "test": "node ./test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^7.1.1",
        "@types/mocha": "^7.0.1",
        "@types/node": "^12.11.7",
        "@types/vscode": "^1.42.0",
        "eslint": "^6.8.0",
        "glob": "^7.1.6",
        "mocha": "^7.0.1",
        "typescript": "^3.7.5",
        "vscode-test": "^1.3.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/AyuSharma-dev/Jaspex_Vscode"
    },
    "dependencies": {
        "clipboardy": "^2.2.0",
        "unirest": "^0.6.0"
    }
}
